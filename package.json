  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "author": "Jonathan Chow",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanchowjh/ethers-sdk.git"
  },
  "keywords": [
    "hardhat",
    "sdk",
    "ethers",
    "ethereum"
  ],
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@openzeppelin/contracts": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "hardhat": "^2.12.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-solidity": "^1.1.0",
    "typedoc": "^0.23.24",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "scripts": {
    "build": "chmod +x ./shell/build.sh && ./shell/build.sh",
    "version": "chmod +x ./shell/version.sh && ./shell/version.sh",
    "lint": "eslint .",
    "prettier": "prettier --write --ignore-unknown **/*",
    "clean": "rm -rf dist",
    "docs": "chmod +x ./shell/docs.sh && ./shell/docs.sh",
    "chain_localhost": "ts-node ./scripts/main.ts chain_localhost",
    "chain_goerli": "ts-node ./scripts/main.ts chain_goerli",
    "chain_read": "ts-node ./scripts/main.ts chain_read",
    "deploy": "ts-node ./scripts/deploy.ts"
  },
  "lint-staged": {
    "**/*.ts": "npm run lint",
    "**/*.*": "prettier --write --ignore-unknown"
  }
}
